{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "restaurant management",
    "version": "0.1.0",
    "identifier": "com.huy1235588.restaurantmanagement",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "restaurant management",
                "maximized": true,
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": ["icons/logo.png", "icons/logo-mini.png", "icons/icon.ico"]
    }
}
